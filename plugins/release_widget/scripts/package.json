{
  "author": "Enalean Team",
  "name": "tuleap-releasewidget",
  "version": "1.0.0",
  "homepage": "https://www.enalean.com",
  "license": "GPL-2.0+",
  "private": true,
  "dependencies": {
    "vue": "^2.6.10",
    "vue-gettext": "^2.1.5"
  },
  "config": {
    "bin": "../../../node_modules/.bin/"
  },
  "scripts": {
    "build": "rm -rf ../../../src/www/assets/releasewidget/scripts && NODE_ENV=production $npm_package_config_bin/webpack --mode=production",
    "watch": "NODE_ENV=watch $npm_package_config_bin/concurrently --raw --kill-others '$npm_package_config_bin/webpack --watch --mode=development'",
    "extract-gettext-cli": "$npm_package_config_bin/gettext-extract --attribute v-translate $(find releasewidget/src/components/ -type f -name '*.vue')"
  }
}
