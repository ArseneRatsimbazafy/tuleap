    "vue-gettext": "^2.1.10",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.1.0"
  },
  "config": {
    "bin": "../../node_modules/.bin"
  },
  "scripts": {
