  "private": true,
  "dependencies": {
    "@tuleap/core": "file:../../src",
    "javascript-time-ago": "^1.0.31",
    "node-gettext": "^3.0.0",
    "prismjs": "^1.21.0",
    "sprintf-js": "^1.1.2",
