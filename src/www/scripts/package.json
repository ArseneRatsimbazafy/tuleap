{
  "name": "tuleap-core",
  "version": "0.1.0",
  "description": "Javascript code used in Tuleap core",
  "author": "Enalean Team",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "ckeditor": "4.11.1",
    "css.escape": "^1.5.1",
    "d3-selection": "^1.3.0",
    "d3-shape": "^1.2.0",
    "d3-transition": "^1.1.1",
    "dompurify": "^1.0.10",
    "dragula": "^3.7.2",
    "get-dataset": "^0.0.4",
    "keymaster": "^1.6.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mustache": "^2.3.0",
    "node-gettext": "^2.0.0",
    "phptomoment": "0.0.2",
    "pretty-kibibytes": "^4.0.4",
    "smooth-scrollbar": "^7.4.1",
    "sprintf-js": "^1.1.1",
    "tus-js-client": "^1.8.0-1",
    "vue": "^2.6.10",
    "vue-gettext": "^2.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "imports-loader": "^0.7.1",
    "load-json-file": "^5.1.0",
    "vuex-mock-store": "0.0.7"
  },
  "config": {
    "bin": "../../../node_modules/.bin"
  },
  "scripts": {
    "build": "NODE_ENV=production $npm_package_config_bin/webpack --mode=production",
    "coverage": "rm -rf ./coverage/* && NODE_ENV=coverage $npm_package_config_bin/karma start ./tuleap/ckeditor/karma.conf.js && $npm_package_config_bin/http-server -os -a 0.0.0.0 -p 9000 coverage/",
    "extract-gettext-cli": "$npm_package_config_bin/gettext-extract --attribute v-translate $(find . -type f -name '*.vue')",
    "test": "NODE_ENV=test $npm_package_config_bin/karma start ./tuleap/ckeditor/karma.conf.js",
    "watch": "NODE_ENV=watch $npm_package_config_bin/concurrently --raw --kill-others '$npm_package_config_bin/webpack --watch --mode=development' '$npm_package_config_bin/karma start ./tuleap/ckeditor/karma.conf.js'"
  }
}
