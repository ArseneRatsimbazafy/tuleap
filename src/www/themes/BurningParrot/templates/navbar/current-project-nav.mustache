{{# current_project_navbar_info_presenter }}
<nav id="current-project-nav">
    <a href="{{ project_link }}" class="current-project-name" title="{{ project_name }}&#10;&#10;{{{ project_privacy }}}">
        {{# project_is_public }}
            <i class="fa fa-unlock current-project-icon"></i>
        {{/ project_is_public }}
        {{^ project_is_public }}
            <i class="fa fa-lock current-project-icon"></i>
        {{/ project_is_public }}

        {{ project_name }}
    </a>
    {{# has_project_flags }}
        <span class="current-project-nav-flag"
              id="current-project-nav-flag"
        >
            {{# glyph }}project-shield{{/ glyph }}
            <span class="current-project-nav-flag-labels">
                {{# project_flags }}
                    <span class="current-project-nav-flag-label">{{ label }}</span>
                {{/ project_flags }}
            </span>
        </span>
        <div class="tlp-popover" id="current-project-nav-flag-popover">
            <div class="tlp-popover-header current-project-nav-flag-popover-header">
                <h3 class="tlp-popover-title current-project-nav-flag-popover-title">{{ project_flags_title }}</h3>
            </div>
            <div class="tlp-popover-body">
                <p>{{ project_flags_description }}
                <ul class="current-project-nav-flag-popover-flags">
                    {{# project_flags }}
                        <li class="current-project-nav-flag-popover-flag">
                            <h2 class="current-project-nav-flag-popover-content-title">
                                {{ label }}
                            </h2>
                            <p class="current-project-nav-flag-popover-content-description">
                                {{ description }}
                            </p>
                        </li>
                    {{/ project_flags }}
                </ul>
            </div>
            <div class="tlp-popover-arrow"></div>
        </div>
    {{/ has_project_flags }}
</nav>
{{/ current_project_navbar_info_presenter }}
